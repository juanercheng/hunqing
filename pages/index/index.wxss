/**index.wxss**/

.dots{  
  width: 156rpx;
  height: 36rpx; 
  display: flex;
  flex-direction: row;
  position: absolute;
  left: 320rpx;
  bottom: 20rpx;
}  
/*未选中时的小圆点样式 */
.dot{  
  width: 26rpx;  
  height: 26rpx;   
  border-radius: 50%;
  margin-right: 26rpx;
  background-color: white;
}  
/*选中以后的小圆点样式  */
.active{  
  width: 26rpx;   
  height: 26rpx;
  background-color: coral;
}
.title-bar{ 
   margin-top: 1.875rem; 
   display: flex; 
   justify-content:space-between;
   margin-left: 0.937rem;
   margin-right: 0.937rem;
   margin-bottom: 1.875rem; 
}
.title-along{
  text-align: center;
}
.title-text{
  font-size:0.75rem;
  display: block;
}
.scroll-H{
  /* width: 100%; */
  /* height:4.87rem;  */
  white-space: nowrap; 
  display: flex; 
   /* margin-left: 0.312rem;   */
  /* margin-right: 0.312rem; */
}
.typeCase{
  width:6.562rem; 
  height:4.687rem;
  background-size:100% 100%; 
  display: inline-block;
  text-align: center;
  color: #fff;
  font-size: 18px;
  line-height: 4.687rem;
}
.alone{
  width:6.562rem; 
  height:4.687rem;
  line-height: 4.687rem;
  background-size:100% 100%; 
  display: inline-block;
  text-align: center;
  color: #fff;
  margin-left:10px;
  font-size: 18px;
  background-image: url('data:image/png;base64,')
}
.scroll-view{
  border:1px #000 solid;
  border-radius: 0.625rem;
  display: inline-block;
  margin-left: 0.625rem;
  background-color: #000;
  align-items: center;
  padding:2rem 1.125rem;
} 
.text-left{
  font-size: 1.125rem;
  font-weight: bold;
  color: #fff;
}  
.text-right{
  font-size: 1.125rem;
  color: #fff;
}
.list{
  margin-left: 0.937rem;
  margin-right:0.937rem; 
}
.recommend-shop{
  margin:2.5rem 0.937rem 1.562rem 0.937rem;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.bg-images{
   text-align: center; 
}
.content{
  width: 100%;
  height: 10.312rem;
}
.list .bg-images .title-content{
  margin-top:20px;
  color:#fff;
  font-size:1rem;
  font-weight: bold;
  letter-spacing:2px;
}
.list .bg-images .title-content-text{
  margin-top:10px;
  color:#a6a8ae;
  font-size:0.75rem;
  
}
.list .location{
  margin-top: 0.937rem;
  display: flex;
  align-items: center;
  color:#a8abb3;
  font-size: 0.75rem;
  
}
.list .location .address{
  margin-left:0.375rem
}
.list .list-details{
  margin-top:1.312rem
}
.list .list-details .list-details-content{
  margin-top:0.937rem;
}
.list .list-details .list-details-content .content-one{
  display:flex;
  align-items:center;
  margin-bottom:0.937rem;
}
.list .list-details .list-details-content .content-one .content-one-right{
  margin-left:0.937rem;
}
.list .list-details .list-details-content .content-one .content-one-right .title{
  font-size:1rem;
  display: -webkit-box; 
  text-overflow: ellipsis;
  overflow : hidden;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.list .list-details .list-details-content .content-one .content-one-right .title-text{
  margin-top:0.625rem;
  font-size:0.75rem;
  color:#a8abb2;
  display: -webkit-box; 
  text-overflow: ellipsis;
  overflow : hidden;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.list .list-details .list-details-content .content-one .content-one-right .price{
  margin-top:1.437rem;
  font-size:0.75rem;
}
.list .list-details .list-details-content .content-one .content-one-right .price .text{
  font-size:0.75rem;
  margin-left:0.375rem;
  color:red;
}
.input{
  margin-left:15px;
  margin-right:15px; 
  height:32px;
  width:92%;
  position: absolute;
  background-color:#fff;
  opacity:0.8;
  top:0;
  margin-top:10px;
  border:2px #ffffff solid;
  border-radius: 16px;
}
.placeholder{
  display: flex;
  align-items: center;
  /* justify-content: center; */
  font-size: 0.75rem;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 14px 14px;
  background-position: 7.1rem 50%;
  padding-left: 8.5rem; 
  color: #787a80;
}
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1em auto;
  line-height: 1em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}