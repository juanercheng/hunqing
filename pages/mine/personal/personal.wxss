  background-color: #f0f2f5;
}
.list{
  display: flex; 
  height: 2.625rem;
  padding-left: 0.937rem;
  padding-right: 0.937rem;
  font-size: 0.875rem;
  align-items: center;
  border-bottom: 1px solid #dcdcdc;
}
.list-other{
  display: flex;
  height: 2.625rem;
  padding-left: 0.65rem;
  padding-right:  0.937rem; 
  font-size: 0.875rem;
