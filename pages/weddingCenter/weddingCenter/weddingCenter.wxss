.type-button{
  position: fixed;
  top: 0;
  width: 100%;
  display: flex;
  justify-content: space-around;
  background-color: #fff;
  border-bottom: 1px #f0f2f5 solid
}
.type-button .type{
  margin-top: 19px;
  font-size: 13px;
  text-align: center;
  margin-bottom: 20px;
}
.type-button .type .text{
  margin-top: 8px;
}
 .top-line-top{
  position: fixed;
  top: 0;
  height: 1rpx;
  width: 100%;
  background-color: #dbdbdb;
} 
.line{
  height: 10px;
  width: 100%;
  background-color: #f0f2f5;
}
.list .order-number{
  margin-top: 20px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 0.812rem;
  color: #a8abb3;
  margin-left: 0.937rem;
  margin-right:0.937rem;
}
.list .commodity{
  background-color: #f7f8fa;
  width: 100%;
  margin-top: 15px;
}
.list .commodity .gray{
  margin-left: 0.937rem;
  margin-right:0.937rem;
  padding-top: 0.937rem;
  padding-bottom: 0.937rem;
  display: flex;
  align-items: center;
}
.list .commodity .gray .title{
  font-size: 0.875rem;
  margin-left: 0.875rem;
}
.list .commodity .gray .title .price{
  margin-top: 18px;
  font-size: 0.75rem;
}
.list .companyNone{
  display: flex;
  align-items: center;
  margin-left: 0.937rem;
  margin-right:0.937rem;
  margin-top: 15px;
  padding-bottom: 0.937rem;
}
.list .company{
  display: flex;
  align-items: center;
  margin-left: 0.937rem;
  margin-right:0.937rem;
  margin-top: 15px;
  border-bottom: 1px #ced3db solid;
  padding-bottom: 0.937rem;
}
.list .company .company-text{
  margin-left:0.812rem;
  font-size: 14px;
}
.list .company .company-phone{
  font-size:11px;
  margin-top:11px;
}
.list .companyNone .company-text{
  margin-left:0.812rem;
  font-size: 14px;
}
.list .companyNone .company-phone{
  font-size:11px;
  margin-top:11px;
}
.list .button{
  margin-left: 0.937rem;
  margin-right:0.937rem;
  margin-top: 15px;
  margin-bottom: 16px;
  display: flex;
  justify-content: flex-end;
}
.list .button .empty{
  height: 34px;
  width: 85px;
  font-size: 0.812rem;
  border: 1px #116699 solid;
  color: #116699;
  border-radius: 5px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.list .button .full{
  height: 34px;
  width: 85px;
  font-size: 0.812rem;
  border: 1px #116699 solid;
  background-color: #116699;
  color: #fff;
  border-radius: 5px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 10px;
}
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1em auto;
  line-height: 1em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}