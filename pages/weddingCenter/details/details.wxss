page{
  height: 100%;
  background-color: #f0f2f5;
}
.image-title{
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 100% 100%; 
  padding: 18px 15px 22px 15px; 
  color: #116699;
}
.line{
  height: 0.937rem;
  background-color: #fff;
  width: 100%
}
.commodity{
  background-color: #f7f8fa;
  width: 100%;
  /* padding-top: 15px; */
}
.commodity .gray{
  padding-left: 0.937rem;
  padding-right:0.937rem;
  padding-top: 0.937rem;
  padding-bottom: 0.7rem;
  display: flex;
  align-items: center;
  
}
.commodity .gray .title{
  font-size: 0.875rem;
  padding-left: 0.875rem;
}
.commodity .gray .title .price{
  padding-top: 18px;
  font-size: 0.75rem;
}
.company{
  display: flex;
  align-items: center;
  padding-left: 0.937rem;
  padding-right:0.937rem;
  padding-top: 20px;
  border-bottom: 1px #ced3db solid;
  padding-bottom: 0.937rem;
  background-color: #fff;
}
.company .company-text{
  padding-left:0.812rem;
  font-size: 14px;
}
.company .company-phone{
  font-size:11px;
  padding-top:11px;
}
.order-details{
  padding-top: 20px;
  padding-left: 0.937rem;
  padding-right:0.937rem;
  padding-bottom: 20px;
  font-size: 0.812rem;
  color: #a8abb3;
  background-color: #fff;
}
.order-details .order-time{
  margin-top: 15px;
}
.order-details .order-timeOne{
  margin-top: 15px;
}
.button{
  width: 100%;
  height: 65px;
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  border-top: 1px #ced3db solid;
  background-color: #fff;
}
.button .empty{
  height: 34px;
  width: 85px;
  font-size: 0.812rem;
  border: 1px #116699 solid;
  color: #116699;
  border-radius: 5px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right:0.937rem; 
}
.button .full{
  height: 34px;
  width: 85px;
  font-size: 0.812rem;
  border: 1px #116699 solid;
  background-color: #116699;
  color: #fff;
  border-radius: 5px;
  display: flex;
  align-items: center;
  justify-content: center;
  /* margin-left: 10px; */
   margin-right:0.937rem; 
}