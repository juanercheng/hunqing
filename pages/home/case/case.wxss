.top{
  position: fixed;
  top: 0;
  width: 100%;
  height:46px;
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-bottom: 1px #dbdbdb solid;
  background-color: #fff;
}

.top .header{
   width: 50%; 
  display: flex;
  height: 30px;
  align-items: center;
  border: 1px solid #dfe3eb;
  border-radius: 15px;
}
.top .header .line{
  height: 10px;
  width: 1px;  
  background-color: #787a80;
  margin-left: 10px;
}
.top .header .input{
  width: 100%;
  font-size: 0.812rem;
  margin-left:10px;
}
.top .top-choice{
  display: flex;
  align-items: center;
}
.top .top-choice .text{
  font-size: 14px;
}
.list{
  margin-left:0.937rem;
  margin-right:0.937rem;
  display: flex;
  align-items: center;
  
  padding-top: 0.937rem;
  padding-bottom: 0.937rem;
  border-bottom: 1px #dcdcdc solid;
}
.list .list-content{
  width:50%;
  margin-left:0.937rem;
}
.list .list-content .list-title{
  font-size: 1rem;
  color: black
}
.list .list-content .list-text{
  font-size: 0.75rem;
  color: #a8abb2;
  margin-top: 10px;
}
.list .list-content .list-all-price{
  font-size: 0.678rem;
  color: #606266;
  margin-top: 15px;
}
.list .list-content .list-price{
  font-size: 0.75rem;
  color: #606266;
  margin-top: 8px;
}
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1em auto;
  line-height: 1em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}
