  font-size:0.75rem;
  
}
.image{
  text-align: center;
  width: 100%;
  height: 10.312rem;
}
.placeholder{
  display: flex;
  align-items: center;
  /* justify-content: center; */
  font-size: 0.75rem;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 14px 14px;
  background-position: 7.1rem 50%;
