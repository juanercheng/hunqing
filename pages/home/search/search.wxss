.header{
  display: flex;
  height: 32px;
  margin-left: 0.973rem;
  margin-right: 0.973rem;
  align-items: center;
  border: 1px solid #dfe3eb;
  border-radius: 16px;
}
.line{
  height: 10px;
  width: 1px;  
  background-color: #787a80;
  margin-left: 10px;
}
.input{
  width: 100%;
  font-size: 0.812rem;
  margin-left:10px;
}
.case{
  margin-top: 1.875rem;
  margin-left: 0.973rem;
  margin-right: 0.973rem; 
}
.case .case-title{
  font-size: 0.812rem;
  color: #a8abb3;
}
.case .more{
  margin-top: 0.937rem;
  background-color: #f2f4f8;
  font-size: 0.75rem;
  padding-top: 0.75rem;
  padding-bottom: 0.75rem;
  text-align: center;
  color: #aaadb3;
}
.case-list{
  display: flex;
  align-items: center;
  
  padding-top: 0.937rem;
  padding-bottom: 0.937rem;
  border-bottom: 1px #dcdcdc solid;
}
.case-list .case-list-content{
  width:50%;
  margin-left:0.937rem;
}
.case-list .case-list-content .case-list-title{
  font-size: 1rem;
  color: black;
  display: -webkit-box; 
  text-overflow: ellipsis;
  overflow : hidden;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.case-list .case-list-content .case-list-text{
  font-size: 0.75rem;
  color: #a8abb2;
  margin-top: 10px;
  display: -webkit-box; 
  text-overflow: ellipsis;
  overflow : hidden;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.case-list .case-list-content .case-list-all-price{
  font-size: 0.678rem;
  color: #606266;
  margin-top: 15px;
}
.case-list .case-list-content .case-list-price{
  font-size: 0.75rem;
  color: #606266;
  margin-top: 8px;
}
.list{
  margin-top:0.937rem;
  border-bottom:1px #dcdcdc solid;
}
.list .location{
  color:#afd1b9;
  font-size: 0.75rem;
  display: flex;
  align-items: center;
  margin-top: 0.937rem;
  margin-bottom: 1.25rem;
}
.list .location .location-text{
  margin-left: 5px;
}
.title-content{
  margin-top:20px;
  color:#fff;
  font-size:1rem;
  font-weight: bold;
  letter-spacing:2px;
}
.title-content-text{
  margin-top:10px;
  color:#a6a8ae;
  font-size:0.75rem;
}
.image{
  text-align: center;
  width: 100%;
  height: 10.312rem;
}